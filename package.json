  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "husky": "^9.0.11",
    "style-loader": "^4.0.0"
  },
  "dependencies": {
    "typescript": "^5.4.5"
  }
}
