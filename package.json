    "docker:server":"cd server && npm run docker:build",
    "kill:3020": "npx kill-port 3020",
    "generate-changelog": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s --grep '(feat|fix|bug)'",
    "prepare": "husky install"
  },
  "keywords": [],
