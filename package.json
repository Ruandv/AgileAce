{
  "name": "agile-ace",
  "version": "1.0.0",
  "description": "AgileAce is a planning poker-like application. This repository is divided into two main components: the Client and the Server.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:server":"cd server && npm start",
